// ERD module that contains `users` and `posts` tables.
erd posts {
  define users {
    id int PK,
    uuid uuid,
    email text,
    text text,
    about_html text,
    created_at timestamp,
  }
  
  define posts {
    id int PK,
    uuid uuid,
    title text,
    content text,
    created_at timestamp,
    created_by int FK,
  }
  
  // posts.FK refers users.PK
  posts.created_by o--o users.id
}